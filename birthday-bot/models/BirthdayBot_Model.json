{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "BirthdayBot",
    "version": "6",
    "intents": [
      {
        "name": "selectChannel",
        "version": "4",
        "fulfillmentActivity": {
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "I want to select a new channel"
        ],
        "slots": [
          {
            "sampleUtterances": [],
            "slotType": "slackChannel",
            "slotTypeVersion": "1",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Required",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Which channel?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 1,
            "name": "channel"
          }
        ]
      },
      {
        "name": "Greeting",
        "version": "8",
        "fulfillmentActivity": {
          "type": "ReturnIntent"
        },
        "sampleUtterances": [
          "Hi",
          "Hello",
          "Good morning"
        ],
        "slots": [],
        "followUpPrompt": {
          "prompt": {
            "messages": [
              {
                "groupNumber": 1,
                "contentType": "PlainText",
                "content": "Hi, I'm BirthdayBot!"
              },
              {
                "groupNumber": 2,
                "contentType": "PlainText",
                "content": "Can I help you?"
              }
            ],
            "maxAttempts": 3
          },
          "rejectionStatement": {
            "messages": [
              {
                "groupNumber": 1,
                "contentType": "PlainText",
                "content": "Ok. Thank you. Have a great day!"
              }
            ]
          }
        }
      },
      {
        "name": "GetBirthdays",
        "version": "9",
        "fulfillmentActivity": {
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "I want birthdays of today"
        ],
        "slots": [],
        "conclusionStatement": {
          "messages": [
            {
              "groupNumber": 1,
              "contentType": "PlainText",
              "content": "I will return birthdays! The birthday of today is @someone!"
            }
          ]
        }
      }
    ],
    "slotTypes": [
      {
        "description": "Available Slack channels",
        "name": "slackChannel",
        "version": "1",
        "enumerationValues": [
          {
            "value": "project-gnalog"
          },
          {
            "value": "gnalog-team"
          }
        ],
        "valueSelectionStrategy": "ORIGINAL_VALUE"
      }
    ],
    "voiceId": "0",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 60,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, can you please repeat that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Goodbye."
        }
      ]
    },
    "detectSentiment": false,
    "enableModelImprovements": false
  }
}